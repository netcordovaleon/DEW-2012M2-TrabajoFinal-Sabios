<h1>Listing queries</h1>

<table>
  <tr>
    <th>Bussiness plan</th>
    <th>Entrepreneur</th>
    <th>Question</th>
    <th>Status</th>
    <th>Bussiness plan</th>
    <th>Entrepreneur</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @queries.each do |query| %>
  <tr>
    <td><%= query.bussiness_plan_id %></td>
    <td><%= query.entrepreneur_id %></td>
    <td><%= query.question %></td>
    <td><%= query.status %></td>
    <td><%= query.bussiness_plan %></td>
    <td><%= query.entrepreneur %></td>
    <td><%= link_to 'Show', query %></td>
    <td><%= link_to 'Edit', edit_query_path(query) %></td>
    <td><%= link_to 'Destroy', query, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

